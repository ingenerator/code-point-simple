{
  "name":        "ingenerator/code-point-simple",
  "description": "Build a simple static file database for geocoding UK postcodes",
  "keywords":    ["geocode", "postcode"],
  "homepage":    "https://github.com/ingenerator/code-point-simple",
  "type":        "library",
  "license":     "BSD",
  "authors":     [
    {
      "name":     "Andrew Coulton",
      "email":    "andrew@ingenerator.com",
      "homepage": "http://www.ingenerator.com"
    },
    {
      "name":     "Matthias Gisder",
      "email":    "matthias@ingenerator.com",
      "homepage": "http://www.ingenerator.com"
    }
  ],
  "require": {
    "php":                                   ">=5.3.3",
    "php-coord/php-coord":                   "~1.1"
  },
  "require-dev": {
      "phpspec/phpspec":                     "dev-master",
      "phpspec/prophecy":                    "1.1.*@dev",
      "bossa/phpspec2-expect":               "dev-master"
  },
  "suggest": {
  },
  "support": {
    "issues": "https://github.com/ingenerator/code-point-simple/issues",
    "source": "https://github.com/ingenerator/code-point-simple"
  },
    "autoload": {
        "psr-0": {
            "Ingenerator": "src/",
            "spec":   ""
        }
    },
    "bin": [
        "bin/code-point-simple.php",
        "bin/geocode.php"
    ],
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist"
    },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
